{
  "expo": {
    "name": "FC Weather",
    "slug": "fc-weather",
    "version": "1.1.12",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#673AB7"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show you the weather."
      },
      "bundleIdentifier": "com.wzl.fcweather",
      "buildNumber": "1.1.12"
    },
    "android": {
      "versionCode": 8,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon2.png",
        "backgroundColor": "#673AB7"
      },
      "package": "com.wzl.fcweather"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
